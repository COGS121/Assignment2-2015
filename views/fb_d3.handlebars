<h1>Likes on Previous Ten Photos</h1>

<div id="chart"></div>

<script src="js/fb_c3_vis.js"></script>

<script>


var chart = c3.generate({
    data: {
        columns: [
            		[
            		{{#each imageArray}}
            			{{this}},
            		{{/each}}
            		]
        ],
        type: 'bar'
    },
    bar: {
        width: {
            ratio: 0.5 // this makes bar width 50% of length between ticks
        }
        // or
        //width: 100 // this makes bar width 100px
    },
    axis: {
        x: {
            type: 'category',
            categories: ['most recent', 'car', 'cat3', 'cat4', 'cat5', 'cat6']
        }
    }
});

</script>
